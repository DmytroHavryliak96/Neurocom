
@model IEnumerable<Neurocom.Models.NetworkType>

@{
    ViewBag.Title = "ManageTypes";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<h3 class="">Менеджмент типів нейронних мереж</h3>

<div class="row">
    <div class="col s8 offset-s2">
        <table class="highlight">
            <thead>
                <tr>
                    <th>Type ID</th>
                    <th>Name</th>
                    <th>Description</th>
                </tr>
            </thead>
            <tbody>
                @foreach (var item in Model)
                {
                    <tr>
                        <td>@item.Id</td>
                        <td>@Html.ActionLink(item.Name, "EditType", new { typeId = item.Id })</td>
                        <td>@item.Description</td>
                    </tr>
                }
            </tbody>
        </table>

    </div>
</div>


<p>
    @Html.ActionLink("Повернутись до списку мереж", "Index", "ManageNetworks");
</p>
